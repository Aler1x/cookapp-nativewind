{
  "expo": {
    "name": "cookapp-nativewind",
    "slug": "cookapp-nativewind",
    "scheme": "cookapp",
    "version": "1.0.0",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/images/icon.png",
    "newArchEnabled": true,
    "orientation": "default",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "output": "static",
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ]
  }
}